APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail Configuration
# Mail driver to use. Options: smtp, sendmail, mailgun, ses, postmark, resend, log, array
# For production, use smtp. For development/testing, use log to write emails to storage/logs/laravel.log
MAIL_MAILER=log
# SMTP encryption scheme. Options: null, tls, ssl
MAIL_SCHEME=null
# SMTP server hostname (e.g., smtp.gmail.com, smtp.mailtrap.io)
MAIL_HOST=127.0.0.1
# SMTP server port (common: 587 for TLS, 465 for SSL, 2525 for Mailtrap)
MAIL_PORT=2525
MAIL_ENCRYPTION=tls
# SMTP username (usually your email address)
MAIL_USERNAME=null
# SMTP password (app-specific password for Gmail, or your email provider password)
MAIL_PASSWORD=null
# Default from email address for all emails sent by the application
MAIL_FROM_ADDRESS="hello@example.com"
# Sender name shown in inbox (use ToyHaven Platform so emails show the brand)
MAIL_FROM_NAME="ToyHaven Platform"
# Mailtrap Configuration (for email testing)
# Uncomment and fill in your Mailtrap credentials to use Mailtrap for email testing
# Get your credentials from: https://mailtrap.io -> Inboxes -> SMTP Settings
# MAILTRAP_HOST=smtp.mailtrap.io
# MAILTRAP_PORT=2525
# MAILTRAP_USERNAME=your_mailtrap_username
# MAILTRAP_PASSWORD=your_mailtrap_password
# MAILTRAP_ENCRYPTION=tls

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Product API Configuration
# Provider options: 'amazon' (Associates), 'canopy', or 'scraperapi' (recommended)
PRODUCT_API_PROVIDER=scraperapi

# ScraperAPI Configuration (Amazon structured data - no Associates account needed)
# Sign up at: https://dashboard.scraperapi.com/signup
SCRAPER_API_KEY=your_scraper_api_key
SCRAPER_API_BASE_URL=https://api.scraperapi.com

# Canopy API Configuration (Alternative)
# Sign up at: https://www.canopyapi.co/
CANOPY_API_KEY=
CANOPY_REST_URL=https://rest.canopyapi.co
CANOPY_GRAPHQL_URL=https://graphql.canopyapi.co

# Amazon Product Advertising API (Alternative - Requires Associates account)
# Only needed if PRODUCT_API_PROVIDER=amazon
AMAZON_ACCESS_KEY=
AMAZON_SECRET_KEY=
AMAZON_PARTNER_TAG=
AMAZON_HOST=webservices.amazon.com
AMAZON_REGION=us-east-1
AMAZON_MARKETPLACE=www.amazon.com

# Google Maps (for trade listing location picker: Places Autocomplete + map)
# Get a key at https://console.cloud.google.com/apis/credentials (enable Maps JavaScript API and Places API)
GOOGLE_MAPS_API_KEY=

VITE_APP_NAME="${APP_NAME}"


